{
  "name": "frontend-auth-boilerplate",
  "version": "1.0.0",
  "description": "Indicates a boilerplate for building authentication features in a frontend project.",
  "type": "module",
  "main": "index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yagnikvadi2003/frontend-auth-boilerplate.git",
    "directory": "src"
  },
  "keywords": [
    "TypeScript",
    "ReactJS",
    "Web Development"
  ],
  "author": {
    "name": "Yagnik Vadi",
    "email": "yagnikvadi8@gmail.com",
    "url": "https://flowcv.me/yagnikvadi2003/"
  },
  "bugs": {
    "url": "https://github.com/yagnikvadi2003/frontend-auth-boilerplate/issues",
    "email": "yagnikvadi8@gmail.com"
  },
  "discussion": "https://github.com/yagnikvadi2003/frontend-auth-boilerplate/discussions",
  "homepage": "https://github.com/yagnikvadi2003/frontend-auth-boilerplate#readme",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./config/webpack.config.js",
    "start:web": "webpack-dev-server --config ./config/webpack.config.js",
    "start:debug:web": "webpack-dev-server --config ./config/webpack.config.js",
    "start:staging:web": "webpack-dev-server --config ./config/webpack.config.js",
    "start:production:web": "webpack serve --config ./config/webpack.prod.js",
    "build:staging": "webpack --config ./config/webpack.prod.js",
    "build:production": "webpack --config ./config/webpack.prod.js",
    "clean": "rm -rf build",
    "clear": "rm -rf build/ node_modules/ pnpm-lock.yaml && pnpm purge cache",
    "commit": "commit",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.4.0",
    "@commitlint/types": "^19.0.3",
    "@svgr/webpack": "^8.1.0",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.4",
    "mini-css-extract-plugin": "^2.9.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "pnpm commitlint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cacheDirectories": [
    "node_modules",
    "pnpm-lock.yaml"
  ],
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=20.16.0"
  },
  "os": [
    "linux"
  ]
}
